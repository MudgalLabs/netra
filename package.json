{
    "name": "netra",
    "description": "Collection of components, icons, hooks and helpers for building React apps at Mudgal Labs",
    "private": false,
    "version": "0.1.14",
    "packageManager": "pnpm@10.12.1",
    "repository": {
        "type": "git",
        "url": "https://github.com/MudgalLabs/netra"
    },
    "main": "./dist/netra.umd.js",
    "module": "./dist/netra.es.js",
    "types": "./dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "import": "./dist/netra.es.js",
            "require": "./dist/netra.umd.js"
        },
        "./styles.css": "./dist/netra.css"
    },
    "engines": {
        "node": ">=22"
    },
    "scripts": {
        "dev": "storybook dev -p 6006",
        "build": "storybook build",
        "build:lib": "tsc && vite build",
        "local": "npm run build:lib && npm pack",
        "lint": "npx @biomejs/biome lint --write ./src",
        "format": "npx @biomejs/biome check --write --organize-imports-enabled=true ./src",
        "test": "vitest",
        "test:cov": "vitest run --coverage",
        "clean": "rm -rf dist",
        "prepublishOnly": "npm run clean && npm run build:lib"
    },
    "dependencies": {
        "@rehookify/datepicker": "^6.6.8",
        "@tanstack/react-table": "^8.21.3",
        "class-variance-authority": "^0.7.1",
        "clsx": "^2.1.1",
        "ldrs": "^1.1.7",
        "lodash": "^4.17.21",
        "qs": "^6.14.0",
        "react": "19.0.0",
        "react-dom": "19.0.0",
        "react-icons": "^5.5.0",
        "recharts": "^2.15.4",
        "sonner": "^2.0.7",
        "tailwind-merge": "^3.3.1"
    },
    "devDependencies": {
        "@babel/core": "7.27.4",
        "@biomejs/biome": "2.0.4",
        "@radix-ui/react-checkbox": "1.3.1",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-popover": "^1.1.14",
        "@radix-ui/react-progress": "^1.1.7",
        "@radix-ui/react-radio-group": "^1.3.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.2.6",
        "@radix-ui/react-tabs": "^1.1.13",
        "@radix-ui/react-toggle": "^1.1.9",
        "@radix-ui/react-toggle-group": "^1.1.10",
        "@radix-ui/react-tooltip": "^1.2.7",
        "@storybook/addon-docs": "9.0.12",
        "@storybook/addon-links": "9.0.12",
        "@storybook/addon-themes": "9.0.12",
        "@storybook/react-vite": "9.0.12",
        "@storybook/test": "8.6.14",
        "@tailwindcss/postcss": "4.1.10",
        "@tailwindcss/vite": "4.1.10",
        "@testing-library/react": "16.3.0",
        "@types/lodash": "^4.17.20",
        "@types/node": "24.0.3",
        "@types/qs": "^6.14.0",
        "@types/react": "19.1.8",
        "@types/react-dom": "19.1.6",
        "@vitejs/plugin-react": "4.5.2",
        "@vitest/coverage-v8": "3.2.4",
        "autoprefixer": "10.4.21",
        "babel-loader": "10.0.0",
        "husky": "9.1.7",
        "jsdom": "26.1.0",
        "json": "11.0.0",
        "lint-staged": "16.1.2",
        "postcss": "8.5.6",
        "prop-types": "15.8.1",
        "storybook": "9.0.12",
        "tailwindcss": "4.1.10",
        "typescript": "5.8.3",
        "vite": "6.3.5",
        "vite-plugin-dts": "4.5.4",
        "vitest": "3.2.4"
    },
    "peerDependencies": {
        "@radix-ui/react-checkbox": "1.3.1",
        "@radix-ui/react-dialog": "^1.1.14",
        "@radix-ui/react-dropdown-menu": "^2.1.15",
        "@radix-ui/react-popover": "^1.1.14",
        "@radix-ui/react-progress": "^1.1.7",
        "@radix-ui/react-radio-group": "^1.3.7",
        "@radix-ui/react-select": "^2.2.5",
        "@radix-ui/react-separator": "^1.1.7",
        "@radix-ui/react-slot": "^1.2.3",
        "@radix-ui/react-switch": "^1.2.6",
        "@radix-ui/react-tabs": "^1.1.13",
        "@radix-ui/react-toggle": "^1.1.9",
        "@radix-ui/react-toggle-group": "^1.1.10",
        "@radix-ui/react-tooltip": "^1.2.7",
        "react": "^19.0.0 || ^19.1.0",
        "react-dom": "^19.0.0 || ^19.1.0"
    },
    "files": [
        "dist"
    ],
    "lint-staged": {
        "*.{js,ts,cjs,mjs,d.cts,d.mts,json,jsonc}": [
            "biome check --files-ignore-unknown=true",
            "biome check --write --no-errors-on-unmatched",
            "biome lint --write --no-errors-on-unmatched --files-ignore-unknown=true",
            "biome format --write --no-errors-on-unmatched"
        ]
    }
}
